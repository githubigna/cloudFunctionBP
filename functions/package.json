    "firebase-functions": "^3.17.2",
    "mercadopago": "^1.5.12",
    "mocha": "^9.2.0",
    "mongodb": "^4.3.1",
    "mongoose": "^6.2.0",
    "ts-mocha": "^9.0.2"
  },
  "devDependencies": {
    "@types/mercadopago": "^1.5.6",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.0",
    "firebase-functions-test": "^0.2.0",
    "typescript": "^3.8.0"
  },
