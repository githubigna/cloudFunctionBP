{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "build-watch": "tsc --watch",
    "start-dev": "firebase functions:config:get > .runtimeconfig.json && firebase emulators:start --only functions",
